{
  "properties": [
    {
      "name": "spring.cloud.easy-config.enabled",
      "type": "java.lang.Boolean",
      "defaultValue": "true",
      "description": "enable easy config."
    },
    {
      "name": "spring.cloud.easy-config.url",
      "type": "java.lang.String",
      "defaultValue": "${spring.cloud.easy-config.url}",
      "description": "the jdbc url."
    },
    {
      "name": "spring.cloud.easy-config.username",
      "type": "java.lang.String",
      "defaultValue": "root",
      "description": "username"
    },
    {
      "name": "spring.cloud.easy-config.password",
      "type": "java.lang.String",
      "defaultValue": "root",
      "description": "password"
    }
  ]
}